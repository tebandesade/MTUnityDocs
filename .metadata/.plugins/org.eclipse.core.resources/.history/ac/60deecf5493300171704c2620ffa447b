package IBMModel1;

import java.util.ArrayList;

public class AlignmentProposal 
{
	private WordPair wPair;
	
	public AlignmentProposal(WordPair wp)
	{
		wPair = wp;
		
	}
	public void addWordPair(WordPair pair)
	{
		wPairs.add(pair);
	}
	public ArrayList<WordPair> getWordPairs()
	{
		return wPairs;
	}
	
}
