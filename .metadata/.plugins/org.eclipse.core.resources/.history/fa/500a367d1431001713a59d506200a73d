package IBMModel1;

public class WordPair 
{
	private int prob;
	
	private String english;
	private String spanish;
	private int count;
	private double likelihoodEst;
	
	public WordPair(String eng,String esp)
	{
		english=eng;
		spanish=esp;
		count=1;
		likelihoodEst=1;
	}
	
	public String getEnglish()
	{
		return english;
	}
	public String getSpanish()
	{
		return spanish;
	}
	public int getCount()
	{
		return count;
	}
	
	public void addCount()
	{
		count++;
	}
	

	
	
	
}
