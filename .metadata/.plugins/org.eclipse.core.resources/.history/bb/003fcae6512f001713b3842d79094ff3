package MT;

public class PairNGram 
{
	HashMap<String,String<String,int>> hm = new HashMap<String,String<String,int>>;
	public PairNGram(String nGramEng, String nGramEsp)
	{
		impimir(nGramEng,nGramEsp);
	}
	
	private void impimir(String text1, String text2)
	{
		System.out.println(text1);
		System.out.println(text2);
	}
}
